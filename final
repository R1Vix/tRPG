<!DOCTYPE html>
<html>
	<head>
		<meta charset="utf-8">
			<style>
            table
            {
                border-collapse: collapse;
            }
            td
            {
                border: 5px solid gray;
            }
            </style>
            <script>
            function start()
            {
                let board="<table>"
                for(let i=0;i<10;i++)
                {
                    board+="<tr>"
                    for(let j=0;j<20;j++)
                    {
                        board+="<td width='100px' height='100px'></td>"
                    }
                    board+="</tr>"
                }
                board+="</table>"
                document.getElementById("display").innerHTML=board
            }
            window.addEventListener("load",start,false)
        </script>
    </head>
    <body>
        <div id="display"></div>
    </body>
</html>
